{
  "name": "taro-ui-vue",
  "version": "1.0.0-alpha.1",
  "description": "UI KIT for Taro/vue",
  "browser": "src/index.js",
  "module": "src/index.js",
  "main": "src/index.js",
  "source": "src/index.js",
  "files": [
    "dist",
    "src",
    "../../README.md"
  ],
  "author": "psaren",
  "repository": {
    "type": "git",
    "url": "git@github.com:psaren/taro-ui-vue.git"
  },
  "templateInfo": {
    "name": "vuex",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "test:analyze": "rollup --config ./config/rollup.config.analyze.js",
    "build:lib": "tsc --project ./tsconfig.build.json",
    "build:lib-dev": "cross-env LIB_ENV=dev rollup --config ./config/rollup.config.js",
    "build:rollup": "rollup --config ./config/rollup.config.js",
    "rollup:test": "node tests/transform-component.js && rollup --config ./config/rollup-test.config.js",
    "clean": "rimraf dist lib",
    "clog": "conventional-changelog -p angular -i CHANGELOG.md -s -r",
    "test": "node ./tests/transform-component.js && cross-env NODE_ENV=test && jest --coverage"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.6",
    "dayjs": "^1.8.27",
    "lodash": "4.17.15",
    "vue": "^2.5.0",
    "vue-class-component": "^7.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/runtime": "^7.7.7",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "@tarojs/components": "3.0.2",
    "@tarojs/mini-runner": "3.0.2",
    "@tarojs/runtime": "3.0.2",
    "@tarojs/taro": "3.0.2",
    "@tarojs/webpack-runner": "3.0.2",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^2.x",
    "@typescript-eslint/parser": "^2.x",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-jsx": "^1.1.2",
    "@vue/test-utils": "^1.0.3",
    "acorn-jsx": "^5.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.0.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-taro": "^3.0.2",
    "babelrc-rollup": "^3.0.0",
    "conventional-changelog-cli": "^2.0.34",
    "copy-dir": "^1.2.0",
    "cross-env": "^7.0.2",
    "csstype": "^2.6.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-taro": "3.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^26.0.1",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-plugin-visualizer": "^4.0.4",
    "rollup-plugin-vue": "^5.1.7",
    "stylelint": "9.3.0",
    "ts-jest": "^26.0.0",
    "typescript": "^3.7.0",
    "vue-eslint-parser": "^7.1.0",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.5.0"
  }
}
