{
  "private": true,
  "name": "taro-ui-vue-demo",
  "version": "1.0.0-alpha.15",
  "description": "demo for Taro-UI-Vue",
  "author": "psaren",
  "repository": {
    "type": "git",
    "url": "git@github.com:psaren/taro-ui-vue.git"
  },
  "templateInfo": {
    "name": "vuex",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "node ./config/fix-build-h5.js && taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "lint": "eslint ./src/pages/**/*.vue",
    "lint:fix": "eslint ./src/pages/**/*.vue --fix"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/components": "3.0.2",
    "@tarojs/runtime": "3.0.2",
    "@tarojs/taro": "3.0.2",
    "classnames": "^2.2.6",
    "dayjs": "^1.8.27",
    "lodash": "^4.17.15",
    "taro-ui-vue": "file:../taro-ui-vue",
    "vue": "^2.5.0",
    "vue-template-compiler": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/mini-runner": "3.0.2",
    "@tarojs/webpack-runner": "3.0.2",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^2.x",
    "@typescript-eslint/parser": "^2.x",
    "babel-core": "^7.0.0-bridge.0",
    "babel-preset-taro": "3.0.2",
    "conventional-changelog-cli": "^2.0.34",
    "copy-dir": "^1.2.0",
    "cross-env": "^7.0.2",
    "csstype": "^2.6.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-taro": "3.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^2.0.5",
    "stylelint": "9.3.0",
    "typescript": "^3.7.0",
    "vue-eslint-parser": "^7.1.0",
    "vue-loader": "^15.9.3"
  }
}
